[Unit]
Description=Discord Bookmark-bot for computer buddies
Documentation=

Wants=network.target
After=network.target

[Service]
User=discordbot
Group=discordbot
#Nice=5
KillMode=none
SuccessExitStatus=0 1

PrivateDevices=true
NoNewPrivileges=true
PrivateTmp=true

WorkingDirectory=/opt/bookmark-bot
ExecStart=/opt/bookmark-bot

[Install]
WantedBy=multi-user.target
